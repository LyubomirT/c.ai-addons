{
    "manifest_version": 3,
    "name": "C.AI Addons",
    "version": "0.2.3",
    "description": "A browser extension that adds some features to character.ai",
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png"
    },
    "action": {
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png"
      },
      "default_title": "Character.AI Addons"
    },
    
    "content_scripts": [
      {
        "matches": ["https://*.character.ai/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  